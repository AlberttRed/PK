; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "EventMovement",
"language": "GDScript",
"path": "res://Logics/event/event_movement.gd"
}, {
"base": "Node",
"class": "EventSystem",
"language": "GDScript",
"path": "res://Logics/event/EventSystem.gd"
}, {
"base": "Node",
"class": "Item",
"language": "GDScript",
"path": "res://Logics/DB/item.gd"
}, {
"base": "Node",
"class": "Pokemon",
"language": "GDScript",
"path": "res://Logics/DB/pokemon.gd"
}, {
"base": "Node",
"class": "Trainer",
"language": "GDScript",
"path": "res://Logics/event/Trainer.gd"
}, {
"base": "Reference",
"class": "Trainer2",
"language": "GDScript",
"path": "res://Logics/event/Trainer_class.gd"
} ]
_global_script_class_icons={
"EventMovement": "",
"EventSystem": "",
"Item": "",
"Pokemon": "",
"Trainer": "",
"Trainer2": ""
}

[application]

config/name="Pokemon3"
run/main_scene="res://World.tscn"
config/icon="res://icon.png"

[autoload]

GLOBAL="*res://Logics/GLOBAL.scn"
CONST="*res://Logics/CONST.gd"
DB="*res://Logics/DB/DB.scn"
GAME_DATA="*res://Logics/GAME_DATA.scn"
AUDIO_DATA="*res://Logics/AUDIO_DATA.scn"
INPUT="*res://Logics/INPUT.scn"
GUI="*res://ui/GUI.tscn"
EVENTS="*res://Logics/event/EventSystem.gd"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=512
window/size/height=384

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
vnen.tiled_tileset_importer={
"apply_offset": false,
"custom_properties": true,
"embed_internal_images": false,
"image_flags": 0,
"post_import_script": "",
"save_tiled_properties": false,
"tile_metadata": true
}
vnen.tiled_importer={
"add_background": false,
"collision_layer": 1,
"custom_properties": true,
"embed_internal_images": false,
"image_flags": 0,
"post_import_script": "",
"save_tiled_properties": false,
"tile_metadata": true,
"uv_clip": true
}

